 }\n\n  logout() {\n    localStorage.removeItem('id_token');\n    window.location.reload();\n  }\n}\n\nexport default new AuthService();\n","C:\\Users\\NEWOWNER\\bootcamp\\DU-VIRT-FSF-PT-08-2022-U-LOLC\\poke app\\PoketrackerApp\\client\\src\\utils\\queries.js",[],"C:\\Users\\NEWOWNER\\bootcamp\\DU-VIRT-FSF-PT-08-2022-U-LOLC\\poke app\\PoketrackerApp\\client\\src\\utils\\mutations.js",[],"C:\\Users\\NEWOWNER\\bootcamp\\DU-VIRT-FSF-PT-08-2022-U-LOLC\\poke app\\PoketrackerApp\\client\\src\\components\\UserPokemonList\\index.js",[],"C:\\Users\\NEWOWNER\\bootcamp\\DU-VIRT-FSF-PT-08-2022-U-LOLC\\poke app\\PoketrackerApp\\client\\src\\components\\PokemonCard\\index.js",["73","74"],"import { React, useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport { useMutation } from '@apollo/client';\nimport { ADD_POKEMON } from '../../utils/mutations';\nimport { QUERY_POKEMONS, QUERY_ME } from '../../utils/queries';\n\nimport Badges from '../Badges/index';\nimport Auth from '../../utils/auth';\n\nimport { Card, Form, Button } from 'react-bootstrap';\n\n\nconst PokemonCard = ({\n    _id,\n    number,\n    pokeName,\n    pokeType,\n    image,\n}) => {\n\n  // console.log(pokemon.pokeName)\n\n  const [shiny, setShiny] = useState(false);\n\n    // console.log(number);\n\n    const [addPokemon, { error }] = useMutation(ADD_POKEMON, {\n        update(cache, { data: { addPokemon } }) {\n            try {\n                const { pokemons } = cache.readQuery({ query: QUERY_POKEMONS });\n\n                cache.writeQuery({\n                    query: QUERY_POKEMONS,\n                    data: { pokemons: [addPokemon, ...pokemons] },\n                });\n            } catch (e) {\n                console.error(e);\n            }\n            const { me } = cache.readQuery({ query: QUERY_ME });\n            cache.writeQuery({\n                query: QUERY_ME,\n                data: { me: { ...me, pokemons: [...me.pokemons, addPokemon] } },\n            });\n        },\n    });\n\n    const handleFormSubmit = async (event) => {\n        event.preventDefault();\n        try {\n            const { data } = await addPokemon({\n                variables: {\n                    number,\n                    pokeName,\n                    pokeType,\n                    image,\n                    pokeUser: Auth.getProfile().data.username,\n                    shiny: shiny\n                },\n            });\n\n            setShiny(false);\n        } catch (err) {\n            console.error(err);\n        }\n    };\n\n    const handleShiny = () => {\n        setShiny(!shiny);\n    };\n\n    // useEffect(() => {\n    //     console.log(shiny);\n    //   });\n\n    return (\n        <div>\n            {Auth.loggedIn() ? (\n                <>\n                    <Card id='pokePoke' style={{ width: '18rem' }}>\n                        <Card.Img variant=\"top\" src={`images/pokemon_sprites/${number}.png`} />\n                        <Card.Body>\n                            <Card.Text id='pokeCard'>{number}</Card.Text>\n                            <Link to={`/thoughts/${_id}`}>\n                                <Card.Title id='pokeCard'>{pokeName}</Card.Title>\n                            </Link>\n                            <Badges\n                                _id={pokeName._id}\n                                pokeType={pokeType}\n                            ></Badges>\n                            <br></br>\n                            <Form>\n                                <Form.Check\n                                    type=\"switch\"\n                                    id=\"custom-switch\"\n                                    onClick={handleShiny}\n                                />\n                            </Form>\n                            <Button variant=\"primary\" id='pokeCard'  onClick={handleFormSubmit} >Add Pokemon</Button>\n                        </Card.Body>\n                    </Card>\n                </>\n            ) : (\n                <p>\n                    You need login to add Pokemon. Please{' '}\n                    <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n                </p>\n            )}\n        </div>\n    );\n};\n\nexport default PokemonCard;","C:\\Users\\NEWOWNER\\bootcamp\\DU-VIRT-FSF-PT-08-2022-U-LOLC\\poke app\\PoketrackerApp\\client\\src\\components\\PokemonList\\index.js",[],"C:\\Users\\NEWOWNER\\bootcamp\\DU-VIRT-FSF-PT-08-2022-U-LOLC\\poke app\\PoketrackerApp\\client\\src\\components\\Badges\\index.js",[],{"ruleId":"75","severity":1,"message":"76","line":41,"column":1,"nodeType":"77","endLine":41,"endColumn":34},{"ruleId":"78","severity":1,"message":"79","line":27,"column":26,"nodeType":"80","messageId":"81","endLine":27,"endColumn":31},{"ruleId":"78","severity":1,"message":"82","line":50,"column":21,"nodeType":"80","messageId":"81","endLine":50,"endColumn":25},"import/no-anonymous-default-export","Assign instance to a variable before exporting as module default","ExportDefaultDeclaration","no-unused-vars","'error' is assigned a value but never used.","Identifier","unusedVar","'data' is assigned a value but never used."]
