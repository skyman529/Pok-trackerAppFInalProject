inglePokemon.js",[],"/Users/faithvaz/Documents/BOOTCAMP/PROJECTS/Project 3/PoketrackerApp/client/src/pages/Profile.js",[],"/Users/faithvaz/Documents/BOOTCAMP/PROJECTS/Project 3/PoketrackerApp/client/src/components/Header/index.js",[],"/Users/faithvaz/Documents/BOOTCAMP/PROJECTS/Project 3/PoketrackerApp/client/src/components/Footer/index.js",[],"/Users/faithvaz/Documents/BOOTCAMP/PROJECTS/Project 3/PoketrackerApp/client/src/utils/auth.js",["73"],"import decode from 'jwt-decode';\n\nclass AuthService {\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  loggedIn() {\n    const token = this.getToken();\n    // If there is a token and it's not expired, return `true`\n    return token && !this.isTokenExpired(token) ? true : false;\n  }\n\n  isTokenExpired(token) {\n    // Decode the token to get its expiration time that was set by the server\n    const decoded = decode(token);\n    // If the expiration time is less than the current time (in seconds), the token is expired and we return `true`\n    if (decoded.exp < Date.now() / 1000) {\n      localStorage.removeItem('id_token');\n      return true;\n    }\n    // If token hasn't passed its expiration time, return `false`\n    return false;\n  }\n\n  getToken() {\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    localStorage.removeItem('id_token');\n    window.location.reload();\n  }\n}\n\nexport default new AuthService();\n","/Users/faithvaz/Documents/BOOTCAMP/PROJECTS/Project 3/PoketrackerApp/client/src/components/PokemonCard/index.js",["74","75","76"],"import { React, useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport { useMutation } from '@apollo/client';\nimport { ADD_POKEMON } from '../../utils/mutations';\nimport { QUERY_POKEMONS, QUERY_ME } from '../../utils/queries';\n\nimport Badges from '../Badges/index';\nimport Auth from '../../utils/auth';\n\nimport { Card, Form, Button } from 'react-bootstrap';\n\n\nconst PokemonCard = ({\n    _id,\n    number,\n    pokeName,\n    pokeType,\n    image,\n}) => {\n\n    // console.log(pokemon.pokeName)\n\n    const [shiny, setShiny] = useState(false);\n\n    // console.log(number);\n\n    const [addPokemon, { error }] = useMutation(ADD_POKEMON, {\n        update(cache, { data: { addPokemon } }) {\n            try {\n                const { pokemons } = cache.readQuery({ query: QUERY_POKEMONS });\n\n                cache.writeQuery({\n                    query: QUERY_POKEMONS,\n                    data: { pokemons: [addPokemon, ...pokemons] },\n                });\n            } catch (e) {\n                console.error(e);\n            }\n            const { me } = cache.readQuery({ query: QUERY_ME });\n            cache.writeQuery({\n                query: QUERY_ME,\n                data: { me: { ...me, pokemons: [...me.pokemons, addPokemon] } },\n            });\n        },\n    });\n\n    const handleFormSubmit = async (event) => {\n        event.preventDefault();\n        try {\n            const { data } = await addPokemon({\n                variables: {\n                    number,\n                    pokeName,\n                    pokeType,\n                    image,\n                    pokeUser: Auth.getProfile().data.username,\n                    shiny: shiny\n                },\n            });\n\n            setShiny(false);\n            {\n                alert(\"Added to collection!\");\n            }\n        } catch (err) {\n            console.error(err);\n        }\n    };\n\n    const handleShiny = () => {\n        setShiny(!shiny);\n    };\n\n    // useEffect(() => {\n    //     console.log(shiny);\n    //   });\n\n    return (\n        <div>\n            {Auth.loggedIn() ? (\n                <>\n                    <Card style={{ width: '18rem' }} id=\"border-golden\">\n                        <Button variant=\"primary\" id=\"pokeCard\" className=\"bg-red\" onClick={handleFormSubmit} >Add Pokemon</Button>\n                        <Card.Text id='pokeCard' className=\"p-2\">{number}</Card.Text>\n                        <Card.Img variant=\"top\" src={`images/pokemon_sprites/${number}.png`} />\n                        <Card.Body>\n                            <Card.Title id='pokeCenter' className='cardTitle py-2 text-blue'>{pokeName}</Card.Title>\n                            <Badges\n                                _id={pokeName._id}\n                                pokeType={pokeType}\n                            ></Badges>\n                            <Form id=\"pokeText\" className=\"px-1 pt-3\">Shiny:\n                                <Form.Check\n                                    inline\n                                    type=\"switch\"\n                                    id=\"custom-switch\"\n                                    label=\"\"\n                                    onClick={handleShiny}\n                                />\n                            </Form>\n                        </Card.Body>\n                    </Card>\n                </>\n            ) : (\n                <p>\n                    You need login to add Pokemon. Please{' '}\n                    <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n                </p>\n            )}\n        </div>\n    );\n};\n\nexport default PokemonCard;","/Users/faithvaz/Documents/BOOTCAMP/PROJECTS/Project 3/PoketrackerApp/client/src/utils/queries.js",[],"/Users/faithvaz/Documents/BOOTCAMP/PROJECTS/Project 3/PoketrackerApp/client/src/utils/mutations.js",[],"/Users/faithvaz/Documents/BOOTCAMP/PROJECTS/Project 3/PoketrackerApp/client/src/components/UserPokemonList/index.js",[],"/Users/faithvaz/Documents/BOOTCAMP/PROJECTS/Project 3/PoketrackerApp/client/src/components/PokemonList/index.js",["77","78"],"//This component is to render the Poke Cards List to add (data labels may change!) -Faith\n\nimport React from 'react';\nimport { Link } from 'react-router-dom';\n\nimport PokemonCard from '../PokemonCard/index';\nimport Auth from '../../utils/auth';\n\n// import image from '../../images/pokemon_sprites/6.png';\n\nimport { Col, Row, Card, Form, Button } from 'react-bootstrap';\n\n\nconst PokemonList = ({ pokemons }) => {\n  if (pokemons) {\n    console.log(pokemons);\n  }\n\n  return (\n    <div>\n      {Auth.loggedIn() ? (\n        <>\n          <Row className=\"g-4 justify-content-md-center\">\n            {pokemons &&\n              pokemons.map((pokemon) => (\n                <Col key={pokemon._id}>\n                  <PokemonCard\n                    key={pokemon._id}\n                    number={pokemon.number}\n                    pokeName={pokemon.pokeName}\n                    pokeType={pokemon.pokeType}\n                    image={pokemon.image}\n                    _id={pokemon._id}\n                  />\n                </Col>\n              ))}\n          </Row>\n        </>\n      ) : (\n        <>\n          <Row className=\"justify-content-md-center\" >\n          <Col>\n            <Card id=\"border-golden\">\n              <Card.Body>\n                <Card.Title id='pokeCenter' className='cardTitle py-2 text-red'>You need login to add Pokemon. Please{' '}\n                  <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n                </Card.Title>\n              </Card.Body>\n            </Card>\n          </Col>\n          </Row>\n        </>\n  )\n}\n    </div >\n  );\n};\n\nexport default PokemonList;\n\n\n","/Users/faithvaz/Documents/BOOTCAMP/PROJECTS/Project 3/PoketrackerApp/client/src/components/Badges/index.js",[],{"ruleId":"79","severity":1,"message":"80","line":41,"column":1,"nodeType":"81","endLine":41,"endColumn":34},{"ruleId":"82","severity":1,"message":"83","line":27,"column":26,"nodeType":"84","messageId":"85","endLine":27,"endColumn":31},{"ruleId":"82","severity":1,"message":"86","line":50,"column":21,"nodeType":"84","m