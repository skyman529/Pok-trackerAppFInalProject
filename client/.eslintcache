\n        event.preventDefault();\n        try {\n            const { data } = await addPokemon({\n                variables: {\n                    number,\n                    pokeName,\n                    pokeType,\n                    image,\n                    pokeUser: Auth.getProfile().data.username,\n                    shiny: shiny\n                },\n            });\n\n            setShiny(false);\n        } catch (err) {\n            console.error(err);\n        }\n    };\n\n    const handleShiny = () => {\n        setShiny(!shiny);\n    };\n\n    // useEffect(() => {\n    //     console.log(shiny);\n    //   });\n\n    return (\n        <div>\n            {Auth.loggedIn() ? (\n                <>\n                    <Card style={{ width: '18rem' }}>\n                        <Card.Img variant=\"top\" src={`images/pokemon_sprites/${number}.png`} />\n                        <Card.Body>\n                            <Card.Text id='pokeCard'>{number}</Card.Text>\n                            <Link to={`/thoughts/${_id}`}>\n                                <Card.Title id='pokeCard'>{pokeName}</Card.Title>\n                            </Link>\n                            <Badges\n                                _id={pokeName._id}\n                                pokeType={pokeType}\n                            ></Badges>\n                            <br></br>\n                            <Form>\n                                <Form.Check\n                                    type=\"switch\"\n                                    id=\"custom-switch\"\n                                    onClick={handleShiny}\n                                />\n                            </Form>\n                            <Button variant=\"primary\" id='pokeCard'  onClick={handleFormSubmit} >Add Pokemon</Button>\n                        </Card.Body>\n                    </Card>\n                </>\n            ) : (\n                <p>\n                    You need login to add Pokemon. Please{' '}\n                    <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n                </p>\n            )}\n        </div>\n    );\n};\n\nexport default PokemonCard;","C:\\Users\\cyoun\\Documents\\GitHub\\PoketrackerApp\\client\\src\\utils\\mutations.js",[],"C:\\Users\\cyoun\\Documents\\GitHub\\PoketrackerApp\\client\\src\\utils\\auth.js",["77"],"import decode from 'jwt-decode';\n\nclass AuthService {\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  loggedIn() {\n    const token = this.getToken();\n    // If there is a token and it's not expired, return `true`\n    return token && !this.isTokenExpired(token) ? true : false;\n  }\n\n  isTokenExpired(token) {\n    // Decode the token to get its expiration time that was set by the server\n    const decoded = decode(token);\n    // If the expiration time is less than the current time (in seconds), the token is expired and we return `true`\n    if (decoded.exp < Date.now() / 1000) {\n      localStorage.removeItem('id_token');\n      return true;\n    }\n    // If token hasn't passed its expiration time, return `false`\n    return false;\n  }\n\n  getToken() {\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    localStorage.removeItem('id_token');\n    window.location.reload();\n  }\n}\n\nexport default new AuthService();\n","C:\\Users\\cyoun\\Documents\\GitHub\\PoketrackerApp\\client\\src\\components\\UserPokemonList\\index.js",[],"C:\\Users\\cyoun\\Documents\\GitHub\\PoketrackerApp\\client\\src\\components\\PokemonList\\index.js",[],"C:\\Users\\cyoun\\Documents\\GitHub\\PoketrackerApp\\client\\src\\components\\Badges\\index.js",[],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":27,"column":26,"nodeType":"84","messageId":"85","endLine":27,"endColumn":31},{"ruleId